code = "shader_type canvas_item;

uniform sampler2DArray tex:filter_linear_mipmap;

void fragment() {
	if(UV.x<0.5&&UV.y<0.5){
		COLOR = textureLod(tex,vec3(UV*2.0,0),0);
	} else if(UV.x>=0.5&&UV.y<0.5){
		vec2 uv_ofs = UV;
		uv_ofs.x-=0.5;
		COLOR = textureLod(tex,vec3(uv_ofs*2.0,1),1);
	}else if (UV.x<0.5&&UV.y>=0.5){
		vec2 uv_ofs = UV;
		uv_ofs.y-=0.5;
		COLOR = texture(tex,vec3(uv_ofs*2.0,2));
	} else if (UV.x>=0.5&&UV.y>=0.5){
		vec2 uv_ofs = UV;
		uv_ofs-=0.5;
		COLOR = texture(tex,vec3(uv_ofs*2.0,3),2);
	}
}

